.macondo-manager(id="#{_id}")
    p.manager--info
        strong model: 
        span.modelName #{_modelName} 
        strong id: 
        span.id= _id
    p.manager--links    
        a.manager--links--new(href="/edit/#{_modelName}/new") New #{_modelName}
        |  
        a.manager--links--edit(href="/edit/#{_modelName}/#{_id}") Edit
        |  
        a.manager--links--delete(href="/delete/#{_modelName}/#{_id}") Delete
        
    script(type="text/javascript").
        if ('undefined' === typeof jQuery) {
            document.write('<script src=' +
            ('__proto__' in {} ? '/zepto' : '/jquery') +
            '.min.js><\/script>')
        }
        
    script(src="/datepickr.js", type="text/javascript")
    script(src="/tinymce/tinymce.min.js", type="text/javascript")
    script(src="/macondo/manager-controller.js", type="text/javascript")
